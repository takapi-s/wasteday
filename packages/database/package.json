{
  "name": "@wasteday/database",
  "version": "0.1.0",
  "description": "Database package for Wasteday application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "generate-types": "supabase gen types typescript --local > src/types/supabase.ts",
    "migrate": "supabase db push",
    "reset": "supabase db reset",
    "start": "supabase start",
    "stop": "supabase stop",
    "seed": "tsx src/seed/index.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "supabase": "^1.123.4",
    "tsx": "^4.20.5",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "supabase",
    "database",
    "typescript",
    "wasteday"
  ],
  "author": "",
  "license": "MIT"
}